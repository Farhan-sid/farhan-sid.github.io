  <script>
document.addEventListener('DOMContentLoaded', function() {
    // Function to check if the email is already in local storage
    function isEmailSubscribed(email) {
        var subscribedEmails = JSON.parse(localStorage.getItem('subscribedEmails')) || [];
        console.log('Subscribed emails:', subscribedEmails);
        return subscribedEmails.includes(email);
    }

    function displayErrorMessage(message) {
        var errorElement = document.querySelector('.shopify-error');
        if (!errorElement) {
            // If the error element doesn't exist, create it
            errorElement = document.createElement('div');
            errorElement.className = 'shopify-error';
            document.querySelector('.mc4wp-response.SANIAAA').appendChild(errorElement);
        }
        errorElement.innerHTML = `<i class="las la-exclamation-triangle"></i> ${message}`;
    }

    // Function to update the success message
    // function updateShopifyMessage(newText) {
    //     var messageElement = document.querySelector('.shopify-message');
    //     if (messageElement) {
    //         messageElement.innerHTML = `<i class="las la-check"></i> ${newText}`;
    //     }
    // }

    // Add event listener to the form submit button
    document.getElementById('submit-btn').addEventListener('click', function(event) {
        var emailInput = document.getElementById('email-input').value.trim();
        console.log('Email input:', emailInput);

        if (!emailInput) {
            // Check if email input is empty and prevent submission
            displayErrorMessage('Please enter an email address.');
            event.preventDefault();
            return;
        }

        if (isEmailSubscribed(emailInput)) {
            event.preventDefault();
            displayErrorMessage('You are already subscribed.');
        } else {
            // If email is not already subscribed, save it to local storage
            var subscribedEmails = JSON.parse(localStorage.getItem('subscribedEmails')) || [];
            subscribedEmails.push(emailInput);
            localStorage.setItem('subscribedEmails', JSON.stringify(subscribedEmails));
            console.log('Email saved:', subscribedEmails); // Debugging line

            // Clear the error message if subscription is successful
            var errorElement = document.querySelector('.shopify-error');
            if (errorElement) {
                errorElement.innerHTML = ''; // Clear the error message
            }

            // updateShopifyMessage('Thanks for subscribing!');
        }
    });
});
</script></div>
